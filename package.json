{
    "name": "nest-project",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "npm run test:type && npm run test:unit && npm run test:integration && npm run test:e2e",
        "test:db:migrate": "dotenv -e .env.test -- prisma db push",
        "test:db:setup": "dotenv -e .env.test -- pnpm run db:setup",
        "test:e2e": "jest --runInBand --config ./test/jest.e2e.ts --forceExit",
        "test:e2e:watch": "pnpm run test:e2e -- --watch",
        "test:integration": "jest --runInBand --config ./test/jest.integration.ts",
        "test:integration:watch": "pnpm run test:integration -- --watch",
        "test:type": "tsc --noEmit",
        "test:unit": "jest --config ./test/jest.unit.ts",
        "test:unit:watch": "pnpm run test:unit -- --watch"
    },
    "dependencies": {
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.3.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/graphql": "^12.2.2",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/mapped-types": "*",
        "@nestjs/microservices": "^10.4.15",
        "@nestjs/platform-express": "^10.0.0",
        "@prisma/client": "6.2.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "dotenv": "^16.4.7",
        "kafkajs": "^2.2.4",
        "lodash": "^4.17.21",
        "nest-winston": "^1.9.7",
        "prisma": "^6.2.1",
        "reflect-metadata": "^0.2.0",
        "rxjs": "^7.8.1",
        "winston": "^3.17.0",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@swc/cli": "^0.6.0",
        "@swc/core": "^1.10.7",
        "@swc/jest": "^0.2.37",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.2",
        "@types/lodash": "^4.17.14",
        "@types/node": "^20.3.1",
        "@types/supertest": "^6.0.0",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "eslint": "^8.0.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.7.0",
        "prettier": "^3.0.0",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    },
    "prisma": {
        "schema": "database/identity/prisma/schema.prisma"
    }
}